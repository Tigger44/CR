#Duration
finalTime = 100

#Step size
t = 1e-3

#Precision
err = 1e-11

charge = 1.6e-19
mass = 1.67e-27

pos(x) = 1e5
pos(y) = 1e5
pos(z) = 1e4

velocity(x) = 1e5
velocity(y) = 1e4
velocity(z) = 1e5

magfield(x) = 0
magfield(y) = 0
magfield(z) = 0

#B_0 used in the dipole field
initialB = 7e8

#   0 - constant
#   1 - 1/r
#   2 - 1/r^2
#   3 - dipole
mode = 3

#Adaptative rk4
adapFlag = 1

#Stop simulation if mag field gets too weak
simStop = 1
